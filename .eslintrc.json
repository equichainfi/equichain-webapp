{
    "extends": "next/core-web-vitals",
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-unused-vars": "warn",
        "no-console": "warn",
        "sort-imports": "off",
        "import/order": "off"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
